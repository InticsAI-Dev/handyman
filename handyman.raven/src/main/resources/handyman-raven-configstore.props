  resource_connection_select_stmt="SELECT  *  FROM resource_connection  where active = 'true' AND name = '%s' "

  config_select_stmt="SELECT  * FROM config_entity where id.configTypeId= '%s' and  active = 'true' "
  config_select_stmt_by_name="SELECT  * FROM config_entity where id.configTypeId= '%s' and id.name= '%s' and  active = 'true' "
  config_select_stmt_by_var= "SELECT * FROM config_entity WHERE id.configTypeId=? and id.name=? and id.variable=? AND  active = 'true'  "

  pipeline_audit_insert_stmt="INSERT INTO pipeline_audit (pipeline_id, created_by, created_date, last_modified_by, last_modified_date, context_node, execution_status_id, lambda_name, parent_pipeline_id, parent_pipeline_name, pipeline_name, file_content, host_name, mode_of_execution, pipeline_load_type, relative_path, request_body, thread_name, parent_action_id, parent_action_name) VALUES(?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?)"
  action_audit_insert_stmt="INSERT INTO action_audit (action_id, created_by, created_date, last_modified_by, last_modified_date, context_node, execution_status_id, lambda_name, parent_pipeline_id, parent_pipeline_name, parent_action_id, parent_action_name, pipeline_name, action_name, input_node, pipeline_id,log,execution_group_id) VALUES(?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?,?,?)"
  statement_audit_insert_stmt="INSERT INTO statement_audit (statement_id, action_id, created_by, created_date, last_modified_by, last_modified_date, rows_processed, rows_read, rows_written, time_taken,statement) VALUES(?, ?, ?, ?, ?, ?,?,?,?,?,?)"
